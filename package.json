{
  "name": "discord-bot",
  "description": "DiscordJS Bot written in TypeScript",
  "module": "index.ts",
  "type": "module",
  "author": "https://github.com/CuteNikki",
  "license": "MIT",
  "homepage": "https://github.com/CuteNikki/discord-bot",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CuteNikki/discord-bot.git"
  },
  "bugs": {
    "url": "https://github.com/CuteNikki/discord-bot/issues"
  },
  "scripts": {
    "dev": "npx tsx ./src/index.ts",
    "deploy": "npx tsx ./src/structure/utilities/register.ts",
    "start": "npx tsx ./dist/src/index.js",
    "build": "rm -rf dist && npx tsc --build ./"
  },
  "overrides": {
    "@discordjs/brokers": "dev",
    "@discordjs/builders": "dev",
    "@discordjs/collection": "dev",
    "@discordjs/core": "dev",
    "@discordjs/formatters": "dev",
    "@discordjs/proxy": "dev",
    "@discordjs/rest": "dev",
    "@discordjs/voice": "dev",
    "@discordjs/util": "dev",
    "@discordjs/ws": "dev"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node-os-utils": "^1.3.4"
  },
  "peerDependencies": {
    "typescript": "^5.5.3"
  },
  "dependencies": {
    "@discord-player/extractor": "^4.5.0",
    "@translate-tools/core": "^1.0.0",
    "@types/ms": "^0.7.34",
    "canvacord": "^6.0.2",
    "cron": "^3.1.7",
    "dayjs": "^1.11.12",
    "discord-html-transcripts": "^3.2.0",
    "discord-hybrid-sharding": "^2.2.0",
    "discord-player": "^6.7.0",
    "discord.js": "^14.16.0-dev.1721866227-bf6761a44",
    "i18next": "^23.12.2",
    "i18next-fs-backend": "^2.3.1",
    "mediaplex": "^0.0.9",
    "mongoose": "^8.5.1",
    "ms": "^2.1.3",
    "node-os-utils": "^1.3.7",
    "pino": "^9.3.2",
    "pino-pretty": "^11.2.2",
    "youtube-ext": "^1.1.25"
  }
}
